{
  "name": "vite-vue3-demo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 8888 --open",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "build:online": "vue-tsc --noEmit && vite build --mode production",
    "build:pre": "vue-tsc --noEmit && vite build --mode pre",
    "build:test": "vue-tsc --noEmit && vite build --mode ctest",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "build:inner": "vue-tsc --noEmit && vite build --mode stable"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nat-plus": "^0.2.1",
    "pinia": "^2.0.23",
    "pinia-plugin-persist": "^1.0.0",
    "vant": "^4.1.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.3",
    "terser": "^5.16.5",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vconsole": "^3.15.0",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mpa-plus": "^0.5.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24"
  }
}
